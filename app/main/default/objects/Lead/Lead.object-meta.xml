        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)} 

var records = {!GETRECORDIDS($ObjectType.Lead)}; 
var strIDs=&apos;&apos;; 
if (records[0] == null) 
{ 
alert(&apos;Please select a record&apos;); 
} 
else 
{ 
for (var n=0; n&lt;records.length; n++) 
{ 
if(strIDs == &apos;&apos;) 
strIDs = records[n]; 
else 
strIDs = strIDs + &quot;,&quot; + records[n] ; 
} 
window.location.href = &apos;/apex/MassManager__MassDelete?deleteIds=&apos;+strIDs + &apos;&amp;selectedObject=lead&amp;retURL=&apos;+ window.location.href; 
}</url>
