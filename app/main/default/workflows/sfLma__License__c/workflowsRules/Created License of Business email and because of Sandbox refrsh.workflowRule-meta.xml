<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <rules>
        <fullName>Created License of Business email and because of Sandbox refrsh</fullName>
        <actions>
            <name>Duplicate_lead_license_has_been_created</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Notify_lead_is_created_because_sandbox_refresh</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>End_date_the_license</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_FlosumStatus_to_Sandbox_refresh</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>License created from business email and Lead is created because of sandbox refresh</description>
        <formula>AND(sfLma__Lead__r.Business_Email__c, 
CONTAINS(  TEXT( sfLma__Lead__r.LeadSource ), &quot;-dup-&quot;)
)</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>